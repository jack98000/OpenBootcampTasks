// Section 1
	
public class Main {

	public static int sum(int number1, int number2, int number3){
		return number1 + number2 + number3;
	}
	
	public static void main(String[] args) {		
		System.out.println(sum(1,1,3));
	}
}

// Section 2

public class Car{

	//Attributes
	public int numberDoors = 0;
	  
	//Methods
	//Setters
	public void incrementDoors(int doors){
	    this.numberDoors += doors;
	}
}

public class Main {
	public static void main(String[] args) {
		//Instantiate car object
		Car newCar = new Car();
		System.out.println(newCar.numberDoors);
	
		//Add multiple doors
		for(int i=1; i<5; i++){
			newCar.incrementDoors(1);
			System.out.println(newCar.numberDoors);
		}
	}
}
